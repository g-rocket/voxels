{
	"@IfSelect": {
		"%module": "@MidSelect",
		"controlSource": "$controlSource",
		"lowThreshold": "$value",
		"highThreshold": "$($value + 0.0000001)",
		"lowSource": "$lessSource",
		"highSource": "$moreSource",
		"midSource": "$equalSource"
	},
	"@MidSelect": {
		"%module": "Select",
		"lowSource": {
			"%module": "Select",
			"threshold": "$lowThreshold",
			"lowSource": "$lowSource",
			"highSource": "$midSource",
			"controlSource": "$controlSource"
		},
		"highSource": "$highSource",
		"threshold": "$highThreshold",
		"controlSource": "$controlSource"
	},
	
	
	"@BlockSelect": {
		"%module": "@IfSelect",
		"$blockSource": "blocksWithoutGrass",
		"$source": {
			"%module": "Cache",
			"source": "$source"
		},
		"value": "BlockType.DIRT",
		"moreSource": "$blockSource",
		"lessSource": "$blockSource",
		"equalSource": "dirtGrassSelect",
		"controlSource": "$blockSource"
	},
	"dirtGrassSelect": {
		"%module": "@IfSelect",
		"value": "BlockType.AIR",
		"moreSource": "BlockType.DIRT",
		"lessSource": "BlockType.DIRT",
		"equalSource": "BlockType.GRASS",
		"controlSource": {
			"%module": "TranslateDomain",
			"axisZSource": 1,
			"source": "$blockSource"
		}
	},
	"blocksWithoutGrass": {
		"%module": "Select",
		"lowSource": {
			"%module": "Select",
			"threshold": 0,
			"lowSource": "BlockType.ROCK",
			"highSource": "BlockType.DIRT",
		"controlSource": "$source"
		},
		"highSource": "BlockType.AIR",
		"threshold": 11,
		"controlSource": "$source"
	},
	
	
	"test": {
		"%module": "@BlockSelect",
		"source": "mountainTerrain"
	},
	
	"main": {
		"%module": "@BlockSelect",
		"source": "terrain"
	},
	
	
	"terrain": {
		"%module": "Select",
		"controlSource": "mountainTerrain",
		"lowSource": "mountainTerrain",
		"highSource": "skyTerrain",
		"threshold": 30
	},
	
	"skyTerrain": {
		"%module": "ScaleDomain",
		"scaleZ": 0.03,
		"scaleX": 0.01,
		"scaleY": 0.01,
		"source": {
			"%module": "ScaleOffset",
			"scale": 40,
			"offset": 10,
			"source": {
				"%module": "Cellular",
				"cellularSource": {
					"%module": "CellGen"
				},
				"coefficients": [-1, 1, 0, 0]
			}
		}
	},
	
	"mountainTerrain": {
		"%module": "TranslateDomain",
		"source": {
			"%module": "Gradient",
			"gradient": [0, 0, 0, 0, 0, 1]
		},
		"axisZSource": {
			"%module": "ScaleDomain",
			"scaleZ": 0.1,
			"scaleX": 0.2,
			"scaleY": 0.2,
			"source": {
				"%module": "ScaleOffset",
				"scale": 20,
				"offset": 10,
				"source": {
					"%module": "Fractal",
					"%args": ["FractalType.FBM", "BasisType.GRADIENT", "InterpolationType.QUINTIC"],
					"numOctaves": 8,
					"frequency": 0.2,
					"seed": "$seed"
				}
			}
		}
	}
}