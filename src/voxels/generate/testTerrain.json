{
	"groundGradient": {
		"%module": "Gradient",
		"gradient": [0, 0, 0, 0, 0, 1]
	},
	"mountainTerrain": {
		"%module": "TranslateDomain",
		"source": "groundGradient",
		"axisZSource": {
			"%module": "ScaleDomain",
			"scaleZ": 0.1,
			"scaleX": 0.2,
			"scaleY": 0.2,
			"source": {
				"%module": "ScaleOffset",
				"scale": 20,
				"offset": 10,
				"source": {
					"%module": "Fractal",
					"%args": ["FractalType.FBM", "BasisType.GRADIENT", "InterpolationType.QUINTIC"],
					"numOctaves": 8,
					"frequency": 0.2,
					"seed": "$seed"
				}
			}
		}
	},
	"dirtGrassSelect": {
		"%module": "Select",
		"threshold": 11,
		"lowSource": "BlockType.DIRT",
		"highSource": "BlockType.GRASS",
		"controlSource": {
			"%module": "TranslateDomain",
			"axisZSource": 1,
			"source": "mountainTerrain"
		}
	},
	"test": {
		"%module": "Select",
		"lowSource": {
			"%module": "Select",
			"threshold": 0,
			"lowSource": "BlockType.ROCK",
			"highSource": "dirtGrassSelect",
			"controlSource": "mountainTerrain"
		},
		"highSource": "BlockType.AIR",
		"threshold": 11,
		"controlSource": "mountainTerrain"
	}
}