{
	"groundGradient": {
		"$module": "Gradient",
		"gradient": [0, 0, 0, 0, 0, 1]
	},
	"mountainTerrain": {
		"$module": "TranslateDomain",
		"axisZSource": {
			"#name": "mountainZScale",
			"$module": "ScaleDomain",
			"scaleZ": 0.1,
			"source": {
				"#name": "mountainScale",
				"$module": "ScaleOffset",
				"scale": 0.45,
				"offset": 0.15,
				"source": {
					"#name": "mountainAutoCorrect",
					"$module": "AutoCorrect",
					"$args": [-1, 1],
					"source": {
						"#name": "mountainShapeFractal",
						"$module": "Fractal",
						"$args": ["FractalType.FBM", "BasisType.GRADIENT", "InterpolationType.QUINTIC"],
						"numOctaves": 8,
						"frequency": 1,
						"seed": "$seed"
					},
					"%calculate": []
				}
			}
		},
		"source": "groundGradient"
	},
	"test": {
		"$module": "Select",
		"lowSource": "BlockType.AIR",
		"highSource": "BlockType.ROCK",
		"threshold": 0.5,
		"controlSource": "mountainTerrain"
	}
}